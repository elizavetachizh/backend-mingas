
<h2 class="page-title">Услуги и их описание</h2>
<a href="/admin/admin_description/add-description" class="btn btn-primary">Добавить новое описание</a>
<br><br>

<table class="table table-striped sorting">

    <thead>
      <tr class="home">
          <th>id</th>
                  <th>nameDescription</th>

                  <th>inform</th>
                  <th>edit</th>
                  <th>delete</th>
              </tr>
    </thead>
     <tbody>
            <% description.forEach(function(el){ %>
            <tr>
                <td><%= el.id %></td>
                <td><%= el.nameDescription %></td>

                <td><div style="width: 600px"><%= el.inform %></div></td>

                <td><a href="/admin/admin_description/edit-description/<%= el._id %>">edit</a></td>
                <td><a class="confirmDeletion" href="/admin/admin_description/delete-description/<%= el._id %>">delete</a></td>
            </tr>
            <% }); %>
        </tbody>
</table>
