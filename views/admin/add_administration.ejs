<h2 class="page-title">Добавить административную услугу</h2>
<br><br>
<a href="/admin/admin_administration" class="btn btn-primary">Показать все административные услуги</a>
<br><br>

<form method="post" action="/admin/admin_administration/add-administration" enctype="multipart/form-data">

    <div class="form-group">
        <label for=""> имя</label>
        <textarea  class="form-control" name="uniqueName"  placeholder="uniqueName"><%= uniqueName %></textarea>
    </div>

    <div class="form-group">
        <label for="">maximumImplementationPeriod</label>
        <textarea  class="form-control" name="maximumImplementationPeriod"  placeholder="maximumImplementationPeriod"><%= maximumImplementationPeriod %></textarea>
    </div>

    <div class="form-group">
        <label for="">certificateValidityPeriod</label>
        <textarea  class="form-control" name="certificateValidityPeriod"  placeholder="certificateValidityPeriod"><%= certificateValidityPeriod %></textarea>
    </div>
    <div class="form-group">
        <label for="">boardSize</label>
        <textarea  class="form-control" name="boardSize"  placeholder="boardSize"><%= boardSize %></textarea>
    </div>
    <div class="form-group">
        <label for="">documents</label>
        <textarea id="ta" class="form-control" name="documents"  placeholder="documents"><%= documents %></textarea>
    </div>
    <div class="form-group">
        <label for="">contactInform</label>
        <textarea id="ta"  class="form-control" name="contactInform"  placeholder="contactInform"><%= contactInform %></textarea>
    </div>
    <div class="form-group">
        <label for="">type</label>
        <input type="text" class="form-control" name="type" value="<%= type %>">

    </div>
    <div class="form-group">
        <label for="">typeAdministrativeService</label>
        <input type="text" class="form-control" name="typeAdministrativeService" value="<%= typeAdministrativeService %>">

    </div>

    <button class="btn btn-default">Добавить</button>

    <% if (errors) {%>
        <% errors.forEach(function(error) { %>
            <div class="alert alert-danger">
                <%= error.msg %>
            </div>
        <% });%>
    <% }%>
</form>
