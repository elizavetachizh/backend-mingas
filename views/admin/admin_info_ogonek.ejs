<h2 class="page-title">Информация для Огонька</h2>
<a href="/admin/admin_ogonek/add-info" class="btn btn-primary">Добавить новое</a>
<br><br>
<% if (count > 0) { %>
    <table class="table table-striped sorting">

        <thead>
        <tr class="home">
            <th>Название раздела</th>
            <th>Тип филиала раздела</th>
            <th>edit</th>
            <th>delete</th>
        </tr>
        </thead>
        <tbody>
        <% info.forEach(function(element){ %>
            <tr>
                <td><%= element.title %></td>
                <td><%= element.typeBranch %></td>
                <td><a href="/admin/admin_ogonek/edit-info/<%= element._id %>">edit</a></td>
                <td><a class="confirmDeletion" href="/admin/admin_ogonek/delete-info/<%= element._id %>">delete</a></td>
            </tr>
        <% }); %>
        </tbody>
    </table>
<% } else {%>
    <h3 class="text-center">Нет ни одного созданного поста</h3>
<% }%>