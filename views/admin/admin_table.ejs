<h2 class="page-title">График приема</h2>
<a href="/admin/admin_table/add-table" class="btn btn-primary">Добавить новое</a>
<br><br>
<% if (count > 0) { %>
    <table class="table table-striped sorting">

        <thead>
        <tr class="home">
            <th>имя</th>
            <th>edit</th>
            <th>delete</th>
        </tr>
        </thead>
        <tbody>
        <% table.forEach(function(tabl){ %>
            <tr>
                <td><%= tabl.name %></td>
                <td><a href="/admin/admin_table/edit-table/<%= tabl._id %>">edit</a></td>
                <td><a class="confirmDeletion" href="/admin/admin_table/delete-table/<%= tabl._id %>">delete</a></td>
            </tr>
        <% }); %>
        </tbody>
    </table>
<% } else {%>
    <h3 class="text-center">Нет ни одного созданного поста</h3>
<% }%>