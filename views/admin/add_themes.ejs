<form method="post" action="/admin/admin_themes/add-themes/">

    <div class="form-group">
        <label for="title">Название темы</label>
        <input type="text" class="form-control" id="title" name="title" value="<%= title %>" placeholder="title">
    </div>

    <div class="form-group">
        <label for="">Предложенные вопросы</label>
            <ul class="list-without-dots">
                <% arrayOfAsks.forEach(function(dep){ %>
                    <li><label>
                            <input type="checkbox" name="question" value="<%= dep._id %>" > <%= dep.question %></input>
                        </label></li>
                <% }); %>
            </ul>
    </div>
    <button class="btn btn-default">Добавить</button>

</form>
