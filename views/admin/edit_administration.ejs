<h2 class="page-title">Редактировать административную услугу</h2>
<a href="/admin/admin_administration" class="btn btn-primary">Назад</a>
<br><br>

<form method="post" action="/admin/admin_administration/edit-administration/<%= id %>">

    <div class="form-group">
        <label for="uniqueName">Наименование административной услуги</label>
        <textarea  class="form-control" name="uniqueName" id="uniqueName"  placeholder="uniqueName"><%= uniqueName %></textarea>
    </div>

    <div class="form-group">
        <label for="maximumImplementationPeriod">Максимальный срок осуществления административной процедуры</label>
        <textarea  class="form-control" id="maximumImplementationPeriod" name="maximumImplementationPeriod"  placeholder="maximumImplementationPeriod"><%= maximumImplementationPeriod %></textarea>
    </div>

    <div class="form-group">
        <label for="certificateValidityPeriod">Срок действия документов</label>
        <textarea  class="form-control" id="certificateValidityPeriod" name="certificateValidityPeriod"  placeholder="certificateValidityPeriod"><%= certificateValidityPeriod %></textarea>
    </div>
    <div class="form-group">
        <label for="">Размер платы, взимаемой при осуществлении административной процедуры</label>
        <textarea  class="form-control" id="boardSize" name="boardSize"  placeholder="boardSize"><%= boardSize %></textarea>
    </div>
    <div class="form-group">
        <label for="">Необходимые документы</label>
        <textarea  class="form-control" name="documents" id="documents"  placeholder="documents"><%= documents %></textarea>
    </div>
    <div class="form-group">
        <label for="">Контактная информация</label>
        <textarea id="ta"  class="form-control" name="contactInform" id="contactInform"  placeholder="contactInform"><%= contactInform %></textarea>
    </div>
    <div class="form-group">
        <label for="">Для кого административная процедура (1 - физ. лица
            2 - юр. лица)</label>
        <input type="text" class="form-control" id="type" name="type" value="<%= type %>">
    </div>

    <div class="form-group">
        <label for="">Тип адм. услуги для физ.лиц</label>
        <input type="text" class="form-control" id="typeAdministrativeService" name="typeAdministrativeService" value="<%= typeAdministrativeService %>">
    </div>

    <button class="btn btn-default">Сохранить</button>

    <% if (errors) {%>
        <% errors.forEach(function(error) { %>
            <div class="alert alert-danger">
                <%= error.msg %>
            </div>
        <% });%>
    <% }%>
</form>
