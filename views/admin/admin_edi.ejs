<div>
    <h2 class="page-title">Информация ЕДИ</h2>
    <a href="/admin/edi/create" class="btn btn-primary">Добавить новую информацию</a>
    <table class="table table-striped sorting">

    <thead>
    <tr class="home">
        <th>Документ</th>
        <th>Скачать</th>
        <th>Удалить</th>
    </tr>
    </thead>
    <tbody>
    <% files.forEach(function(element){ %>
        <tr>
            <td><%= element.name %></td>
            <td><a href="/admin/edi/files/<%= element.name %>">скачать</a></td>
            <td><a href="/admin/edi/delete/<%= element.id %>">удалить</a></td>
        </tr>
    <% }); %>
    </tbody>
    </table>
</div>