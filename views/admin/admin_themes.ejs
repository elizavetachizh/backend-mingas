
<h2 class="page-title">Управление</h2>
<a href="/admin/admin_themes/add-themes" class="btn btn-primary">Добавить новое вопрос-ответ</a>
<br><br>

<table class="table table-striped sorting">

    <thead>
    <tr class="home">
        <th>Название</th>
        <th>Вопрос-ответ темы</th>
        <th>Редактировать</th>
        <th>Удалить</th>
    </tr>
    </thead>
    <tbody>
    <% themes.forEach(function(el){ %>
        <tr>
            <td><%= el.title %></td>
            <td> <% el.questionAnswer.forEach(function(el){ %> <p><%= el.question %></p> <% }); %></td>

            <td><a href="/admin/admin_themes/edit-themes/<%= el._id %>">edit</a></td>
            <td><a class="confirmDeletion" href="/admin/admin_themes/delete-themes/<%= el._id %>">delete</a></td>
        </tr>
    <% }); %>
    </tbody>
</table>
